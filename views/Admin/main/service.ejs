<div class="main-panel">
    <div class="content-wrapper">
        <div class="page-header">
            <h3 class="page-title">
                LIST OF SERVICE
            </h3>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Admin</a></li>
                    <li class="breadcrumb-item active" aria-current="page">Service</li>
                </ol>
            </nav>
        </div>
        <div class="row">

            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Service</h4>
                        <h4 class="card-title text-right">
                            <a href="/admin/add-service" class="text-primary"> <i class="fa fa-plus "> </i> Add service</a>
                        </h4>
                        <br>

                        <div class="table-responsive">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>
                                            ID
                                        </th>
                                        <th>
                                            Image
                                        </th>
                                        <th>
                                            Name
                                        </th>
                                        <th>
                                            Price
                                        </th>
                                        <th>
                                            Desc
                                        </th>
                                        <th>

                                            Duration
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.map(function(item){%>
                                        <tr>
                                            <td>
                                                <%= item.SERVICE_ID%>
                                            </td>
                                            <td class="py-1">
                                                <img src="<%= item.SERVICE_IMG %>" alt="image" />
                                            </td>
                                            <td>
                                                <%= item.SERVICE_NAME%>
                                            </td>
                                            <td>
                                                <%= item.SERVICE_PRICE%>
                                            </td>
                                            <td>
                                                <%= item.SERVICE_DESC%>
                                            </td>
                                            <td>
                                                <%= item.SERVICE_DUR%>
                                            </td>
                                        </tr>
                                        <%})%>
                                </tbody>


                            </table>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>