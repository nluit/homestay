<div class="main-panel">

    <div class="content-wrapper">
        <div class="page-header">
            <h3 class="page-title">
                LIST OF CUSTOMER
            </h3>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Admin</a></li>
                    <li class="breadcrumb-item active" aria-current="page">List of customer</li>
                </ol>
            </nav>
        </div>
        <div class="card">
            <div class="card-body">
                <h4 class="card-title text-right">
                    <a href="#" class="text-primary"> <i class="fa fa-plus "> </i> Add customer</a>
                </h4>

                <div class="row">


                    <div class="col-12">
                        <div class="table-responsive">
                            <table id="order-listing" class="table">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>NAME </th>
                                        <th> GENDER </th>
                                        <th>ADDRESS</th>
                                        <th>EMAIL</th>
                                        <th>BIRTH DAY </th>
                                        <th>PHONE </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.map(function(item) {%>
                                        <tr>
                                            <td>
                                                <%= item.CUS_ID %>
                                            </td>
                                            <td>
                                                <%= item.CUS_NAME %>
                                            </td>
                                            <td>
                                                <%= item.CUS_GENDER %>
                                            </td>
                                            <td>
                                                <%= item.CUS_ADDRESS %>
                                            </td>
                                            <td>
                                                <%= item.CUS_EMAIL %>
                                            </td>
                                            <td>
                                                <%= item.CUS_BIRTH %>
                                            </td>
                                            <td>
                                                <%= item.CUS_PHONE %>
                                            </td>
                                        </tr>
                                        <%})%>
                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br>
        <br>


        <br>
    </div>