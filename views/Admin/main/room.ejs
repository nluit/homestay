<div class="main-panel">

    <div class="content-wrapper">
        <div class="page-header">
            <h3 class="page-title">
                LIST OF ROOM
            </h3>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Admin</a></li>
                    <li class="breadcrumb-item active" aria-current="page">list of room</li>
                </ol>
            </nav>
        </div>
        <div class="card">
            <div class="card-body">
                <h4 class="card-title text-right">
                    <a href="/admin/add-room" class="text-primary"> <i class="fa fa-plus "> </i> Add room</a>
                </h4>
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table id="order-listing" class="table">
                                <thead>
                                    <tr>
                                        <th>ID</th>
                                        <th>IMAGE</th>
                                        <th>ROOM TYPE NAME</th>
                                        <th>FACILITY NAME</th>
                                        <th>ROOM STATUS</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.map(function(item,i){ %>
                                        <tr>
                                            <td>
                                                <%= item.ROOM_ID %>
                                            </td>
                                            <td class="py-1">
                                                <img src="<%= item.ROOM_IMG %>" alt="image" />
                                            </td>

                                            <td>
                                                <%= item.roomtype_name %>
                                            </td>
                                            <td>
                                                <%= item.fac_name %>
                                            </td>
                                            <td>
                                                <%= item.ROOM_STATUS %>
                                            </td>



                                        </tr>
                                        <%})%>


                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br>
        <br>


        <br>
    </div>