<div class="site-section bg-light">
    <div class="container">
        <% if(numpage >= cur ) {%>

            <div class="row">
                <div class="col-md-6 mx-auto text-center mb-5 section-heading">
                    <h2 class="mb-5">Our Rooms</h2>
                </div>
            </div>
            <%}%>
                <% if(numpage < cur) {%>
                    <div class="row">
                        <div class="col-md-6 mx-auto text-center mb-5 section-heading">
                            <h2 class="mb-5">N/A</h2>
                        </div>
                    </div>
                    <%}%>

                        <div class="row">
                            <% data.map(function(item){%>
                                <div class="col-md-6 col-lg-4 mb-5">
                                    <div class="hotel-room text-center">
                                        <a href="/room/<%= item.ROOMTYPE_ID %>" class="d-block mb-0 thumbnail"><img src="/suites/suites/images/img_1.jpg" alt="Image" class="img-fluid"></a>
                                        <div class="hotel-room-body">
                                            <h3 class="heading mb-0">
                                                <a href="#">
                                                    <%= item.ROOMTYPE_NAME %>
                                                </a>
                                            </h3>
                                            <strong class="price"><%= item.ROOMTYPE_PRICE%> Ä‘</strong>
                                        </div>
                                    </div>
                                </div>
                                <%})%>


                        </div>
                        <% if(numpage >= cur ) {%>
                            <div class="row mt-5">
                                <div class="col-md-12 text-center">

                                    <div class="site-block-27">
                                        <ul>
                                            <% if(cur > 1) {%>
                                                <li><a href="/?page=<%= cur-1  %>">&lt;</a></li>
                                                <%}%>
                                                    <% if(cur < 1) {%>
                                                        <li class="disabled"><a disabled href="/?page=<%= cur +1 %>">&lt;</a></li>
                                                        <%}%>
                                                            <% for(let i=0; i< numpage ; i++) {%>
                                                                <% if( cur == i+1 ) {%>
                                                                    <li class="active"><span> <a  href="/?page=<%= i+1 %>"> <%= i+1 %></a></span></li>
                                                                    <%}%>
                                                                        <% if( cur != i+1 ) {%>
                                                                            <li><span> <a href="/?page=<%= i+1 %>"> <%= i+1 %></a></span></li>
                                                                            <%}%>
                                                                                <%}%>
                                                                                    <% if(cur < numpage) {%>
                                                                                        <li><a href="/?page=<%= cur+1  %>">&gt;</a></li>
                                                                                        <%}%>
                                                                                            <% if(cur > numpage) {%>
                                                                                                <li class="disabled"><a disabled href="/?page=<%= cur -1 %>">&gt;</a></li>
                                                                                                <%}%>

                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <%}%>
    </div>
</div>